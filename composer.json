{
  "name": "slouma2000/laravel-webpack-manifest",
  "description": "Package to use WebpackManifestPlugin in laravel",
  "keywords": ["laravel", "webpack", "laravel-webpack", "laravel assets", "assets", "mix"],
  "type": "package",
  "require": {
    "php": ">=7.1",
    "illuminate/support": "^6.0",
    "illuminate/routing": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.1",
    "mockery/mockery": "^0.9.9"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "malyusha",
      "email": "mii18@yandex.ru"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Malyusha\\WebpackAssets\\": "src/"
    },
    "files": [
      "lib.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/AssetTest.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Malyusha\\WebpackAssets\\WebpackAssetsServiceProvider"
      ]
    },
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  }
}
